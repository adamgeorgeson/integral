.menu-item-container
  .menu-item
    - formatted_item = Integral::MenuItemRenderer.new(f.object)
    = render 'item_modal', f:f, formatted_item: formatted_item
    .col.m1
      %i.material-icons.identifier web
    .data.col.m9
      %span.title= formatted_item.title
      %span.url= formatted_item.url
    .actions.col.m2.center
      = link_to "#menu-item-modal-#{f.object.id}", class: 'modal-trigger' do
        %i.material-icons.action edit

