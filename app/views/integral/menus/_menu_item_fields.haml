.menu-item-container
  .menu-item
    .col.m1
      %i.material-icons add
    .data.col.m9
      %span.title= f.object.title
      %span.url= f.object.url
    .actions.col.m2.center
      = link_to "#menu-item-modal-#{f.object.id}", class: 'modal-trigger' do
        %i.material-icons.action edit
      %i.material-icons.action add
  = render 'item_modal', f:f

